# Задача на калькулятор
x=float(input())
y=float(input())
z=(input())
if z==("+"):
    print (x+y)
if z==("-"):
    print (x-y)
if z==("/"): 
    if  y==0:
        print ("Деление на 0!")
    else:
         print (x/y)
      
if z==("*"): 
    print (x*y)
if z==("mod"): 
    if y==0:
        print ("Деление на 0!")
    else:
        print (x%y)
if z==("pow"): 
    print (x**y)
if z==("div"): 
    if y==0: 
        print ("Деление на 0!")
    else:
        print (x//y)
  
